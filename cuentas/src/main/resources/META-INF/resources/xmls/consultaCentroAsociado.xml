		</class-a>
		<class-b>mx.babel.bansefi.banksystem.cuentas.beans.CentroAsociadoBean
		</class-b>
				
		<field>
			<a>codinternouo</a>
			<b>centro</b>      
		</field>
		<field custom-converter="mx.babel.bansefi.banksystem.base.utils.IntegerToDateConverter" custom-converter-param="YYYY">
			<a>fechainicio</a>
			<b>fechaInicio</b>
		</field>
		<field custom-converter="mx.babel.bansefi.banksystem.base.utils.IntegerToDateConverter" >
			<a>fechacrre</a>
			<b>fechaFin</b>
		</field>
		<field>
			<a>codrlacuo</a>
			<b>codRel</b>      
		</field>
	</mapping>
	
</mappings>
