    <field custom-converter="mx.babel.bansefi.banksystem.base.utils.IntegerToDateConverter">
      <a>fechaHasta</a>
      <b>FDESDEHASTAV.stdfecha2</b>
    </field>
    <field>
      <a>fechaAnteriorDesde</a>
      <b>FDESDEHASTAANTERIORV.stdfecha</b>
    </field>
    <field>
      <a>fechaAnteriorHasta</a>
      <b>FDESDEHASTAANTERIORV.stdfecha2</b>
    </field>
    <field>
      <a>horaPlanificada</a>
      <b>horaplanif</b>
    </field>
    <field>
      <a>cuentaBean.entidad</a>
      <b>ACACP.codnrbeen</b>
    </field>
    <field>
      <a>cuentaBean.centro</a>
      <b>ACACP.codcentuo</b>
    </field>
    <field>
      <a>cuentaBean.numeroCuenta</a>
      <b>ACACP.numsecac</b>
    </field>
