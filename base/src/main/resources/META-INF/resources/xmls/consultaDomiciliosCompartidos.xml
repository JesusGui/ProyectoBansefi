		</class-a>
		<class-b>mx.babel.bansefi.banksystem.base.beans.domain.DomicilioCompartidoBean
		</class-b>
		<field>
			<a>nomb50</a>
			<b>nombre</b>
		</field>
		<field>
			<a>idinternope</a>
			<b>idInternoPersona</b>
		</field>
		<field>
			<a>idextpe</a>
			<b>idExternoPersona</b>
		</field>
		<field custom-converter="mx.babel.bansefi.banksystem.base.utils.IntegerToDateConverter">
			<a>fecnctoconstpe</a>
			<b>fechaNacimientoConstitucion</b>
		</field>
		<field>
			<a>codofcnaalta</a>
			<b>codigoOficina</b>
		</field>
		<field>
			<a>codpe</a>
			<b>codigoPersona</b>
		</field>
		<field>
			<a>codidextpers</a>
			<b>codigoIdExternoPersona</b>
		</field>
	</mapping>
</mappings>