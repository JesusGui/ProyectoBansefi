<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<!-- Mapeo de clases -->
	<mapping map-id="convierteCatalogoGeoBeanToDomicilioBean" wildcard="false" map-null="false">
		<class-a>mx.babel.arq.catalogo.beans.CatalogoGeoBean
		</class-a>
		<class-b>mx.babel.bansefi.banksystem.base.beans.domain.DomicilioBean
		</class-b>
		<field>
		 	<a>numArGeo</a>
			<b>numArGeo</b>
		</field>
		<field>
		    <a>nombreComunidadAutonoma</a>
		    <b>estado</b>
		</field>
		<field>
		    <a>codigoComunidadAutonoma</a>
		    <b>codEstado</b>
		</field>
		<field>
		    <a>codigoMunicipio</a>
		    <b>municipio</b>
		</field>	
		<field>
		    <a>nombreMunicipio</a>
		    <b>municipio</b>
		</field>	
		<field>
		    <a>nombrePais</a>
		    <b>pais</b>
		</field>
		<field>
		    <a>codigoPais</a>
		    <b>codPais</b>
		</field>		
	</mapping>
	<mapping map-id="convierteCatalogoGeoBeanToDomicilioTipoBean" wildcard="false" map-null="false">
		<class-a>mx.babel.arq.catalogo.beans.CatalogoGeoBean
		</class-a>
		<class-b>mx.babel.bansefi.banksystem.base.beans.domain.DomicilioTipoBean
		</class-b>
		<field>
		 	<a>numArGeo</a>
			<b>numArGeo</b>
		</field>
		<field>
		    <a>nombreComunidadAutonoma</a>
		    <b>estado</b>
		</field>
		<field>
		    <a>codigoComunidadAutonoma</a>
		    <b>codEstado</b>
		</field>
		<field>
		    <a>codigoMunicipio</a>
		    <b>codMunicipio</b>
		</field>	
		<field>
		    <a>nombreMunicipio</a>
		    <b>municipio</b>
		</field>	
		<field>
		    <a>nombrePais</a>
		    <b>pais</b>
		</field>
		<field>
		    <a>codigoPais</a>
		    <b>codPais</b>
		</field>			
	</mapping>
</mappings>