<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<mapping map-id="resultadosGrupo" trim-strings="true">
		<class-a>mx.babel.bansefi.banksystem.base.webservices.consultagrupoporidexterna.ResponseBansefi.OTRGRLSTTRNO.TRGRLSTEVTZ.TRGRLSTEVTV
		</class-a>
		<class-b>mx.babel.bansefi.banksystem.base.beans.busqueda.GrupoBusquedaBean
		</class-b>
		<field>
			<a>nombgr</a>
			<b>nombre</b>
		</field>
		<field
			custom-converter="mx.babel.bansefi.banksystem.base.utils.IntegerToStringConverter">
			<a>fechaaltagr</a>
			<b>fechaConstitutiva</b>
		</field>
		<field custom-converter-id="obtenerDescripcionCatalogoConverter"
			custom-converter-param="TP_GRP">
			<a>codgrp</a>
			<b>tipoGrupo</b>
		</field>
		<field>
			<a>codgrp</a>
			<b>codGrupo</b>
		</field>
		<field>
			<a>idextgr</a>
			<b>idInterna</b>
		</field>
	</mapping>
</mappings>