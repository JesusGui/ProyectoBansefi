<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<!-- Mapeo de clases -->
	<mapping map-id="centroBusquedaCodigo">
		<class-a>mx.babel.bansefi.banksystem.base.webservices.busquedacodigocentro.ResponseBansefi.OTRCONMINCENTRO2TRN.TRCONMINCENTROEVTZ.UOCENTROE
		</class-a>
		<class-b>mx.babel.bansefi.banksystem.base.beans.busqueda.CentroBusquedaBean
		</class-b>
		<field>
			<a>codinternouo</a>
			<b>codigoCentro</b>
		</field>
		<field>
			<a>nombcentuo</a>
			<b>nombre</b>
		</field>
		<field
			custom-converter="mx.babel.bansefi.banksystem.base.utils.IntegerToStringConverter">
			<a>fechaaltauo</a>
			<b>fechaAlta</b>
		</field>
		<field>
			<a>codecvcent</a>
			<b>estado</b>
		</field>

	</mapping>
	
	<!-- Mapeo de clases -->
	<mapping map-id="centroBusquedaNombre">
		<class-a>mx.babel.bansefi.banksystem.base.webservices.listacentros.ResponseBansefi.OTRLISTNOMBCENTTRNO.TRLISTNOMBCENTEVTZ.UOLAV2
		</class-a>
		<class-b>mx.babel.bansefi.banksystem.base.beans.busqueda.CentroBusquedaBean
		</class-b>
		<field>
			<a>codinternouo</a>
			<b>codigoCentro</b>
		</field>
		<field>
			<a>nombcentuo</a>
			<b>nombre</b>
		</field>
		<field
			custom-converter="mx.babel.bansefi.banksystem.base.utils.IntegerToStringConverter">
			<a>fechaaltauo</a>
			<b>fechaAlta</b>
		</field>
		<field>
			<a>codecvcent</a>
			<b>estado</b>
		</field>

	</mapping>
	
	
</mappings>